<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<!--if you remove parallel parameter from Suit level, it will run in sequancial order -->

	<suite name="Suite" parallel="tests">
		<listeners>
			<listener class-name="Academy.Listeners"/>
			
		</listeners>
		
	

	
	<test  name="validateNavigationBar">
		<classes>
			<class name="Academy.validateNavigationBar"/>
		</classes>
	</test>
	
	<test  name="HomePage">
		<classes>
			
			<class name="Academy.HomePage"/>
			
		</classes>
	</test>
	
	<test name="validateTitle">
		<classes>
			<class name="Academy.validateTitle"/>
		</classes>
	</test>
	<!-- Test -->
</suite> <!-- Suite -->
